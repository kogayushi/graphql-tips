            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

// 変数取得時に null の場合は空文字列を代入し、split でリスト化
def article_user_ids = (vars.get(&quot;article_user_ids&quot;) ?: &quot;&quot;).split(&quot;,&quot;).toList()
def comment_user_ids = (vars.get(&quot;comment_user_ids&quot;) ?: &quot;&quot;).split(&quot;,&quot;).toList()

// `unique()` を適用して重複を削除
def userIds = (article_user_ids + comment_user_ids).findAll { it }.unique().join(&quot;,&quot;)

// JMeter変数に格納
vars.put(&quot;userIds&quot;, userIds)</stringProp>
