            <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(response)

def authorIds = json.collect { it.authorId }

def likedByIds = json.collectMany { it.likedBy ?: [] }

def userIds = (authorIds + likedByIds).unique().join(&quot;,&quot;)

vars.put(&quot;article_user_ids&quot;, userIds)</stringProp>
